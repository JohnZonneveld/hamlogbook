<div class="container">
  
  <header><img src="../images/radiostationsketch.jpg" class="img-responsive main-image"></header>
  <div class="col-md-8 col-md-offset-2">
  <h2>Callsign: <%= @callsign.name %></h2>
    <% if is_logged_in? %>
    <a href="/">Home</a> | <a href="/callsigns/<%=@callsign.id%>/edit">Edit</a> | <a href="/contacts">View contacts</a> | <a href="/logout">Logout </a>
    <br><br>
        <div class="content">
            <p>Callsign: <%= @callsign.name %></p>
            <p>Home QTH: <%= @callsign.gridsquare %><p>
        </div>
    <% else %>
        <a href="/signup">Sign Up </a> | <a href="/login">Log In</a><br><br>
        <i>You have to be logged in to look around</i>

            
        <% end %>
  </div>
</div>
